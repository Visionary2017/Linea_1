<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="layout.Menu_Fragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="25dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageButton
                android:id="@+id/btnActualizarUsuario"
                android:layout_width="56dp"
                android:layout_height="114dp"
                android:layout_marginRight="10dp"
                android:layout_weight="0.15"
                android:background="@null"
                android:paddingBottom="25dp"
                android:src="@drawable/bloggif_59c1419548f48"></ImageButton>

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="-95dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="30dp"
                android:clickable="false"
                android:text="ACTUALIZAR "
                android:textColor="@color/Green"
                android:textSize="15sp"
                android:textStyle="bold"></TextView>

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="-90dp"
                android:layout_marginTop="48dp"
                android:clickable="false"
                android:text="USUARIO"
                android:textColor="@color/Green"
                android:textSize="15sp"
                android:textStyle="bold"></TextView>

            <ImageButton
                android:id="@+id/btnRecargar"
                android:layout_width="56dp"
                android:layout_height="114dp"
                android:layout_marginLeft="20dp"
                android:layout_weight="0.15"
                android:background="@null"
                android:paddingBottom="25dp"
                android:src="@drawable/recargar"></ImageButton>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="-75dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="30dp"
                android:clickable="false"
                android:text="RECARGA"
                android:textColor="@color/Green"
                android:textSize="15sp"
                android:textStyle="bold"></TextView>


        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="25dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageButton
                android:id="@+id/btnConsultarSaldo"
                android:layout_width="56dp"
                android:layout_height="114dp"
                android:layout_marginRight="10dp"
                android:layout_weight="0.15"
                android:background="@null"
                android:paddingBottom="25dp"
                android:src="@drawable/recarga"></ImageButton>

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="-95dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="30dp"
                android:clickable="false"
                android:text="CONSULTAR"
                android:textColor="@color/Green"
                android:textSize="15sp"
                android:textStyle="bold"></TextView>

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="-90dp"
                android:layout_marginTop="48dp"
                android:clickable="false"
                android:text="SALDO"
                android:textColor="@color/Green"
                android:textSize="15sp"
                android:textStyle="bold"></TextView>

            <ImageButton
                android:id="@+id/btnEstaciones"
                android:layout_width="56dp"
                android:layout_height="114dp"
                android:layout_marginLeft="45dp"
                android:layout_weight="0.15"
                android:background="@null"
                android:paddingBottom="25dp"
                android:src="@drawable/estaciones"></ImageButton>

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="-85dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="30dp"
                android:clickable="false"
                android:text="ESTACIONES"
                android:textColor="@color/Green"
                android:textSize="15sp"
                android:textStyle="bold"></TextView>


        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="25dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageButton
                android:id="@+id/btnCalcular_Viaje"
                android:layout_width="56dp"
                android:layout_height="114dp"
                android:layout_marginRight="10dp"
                android:layout_weight="0.15"
                android:background="@null"
                android:paddingBottom="25dp"
                android:src="@drawable/calcular_viaje"></ImageButton>

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="-95dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="30dp"
                android:clickable="false"
                android:text="CALCULAR     "
                android:textColor="@color/Green"
                android:textSize="15sp"
                android:textStyle="bold"></TextView>

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="-90dp"
                android:layout_marginTop="48dp"
                android:clickable="false"
                android:text="VIAJE  "
                android:textColor="@color/Green"
                android:textSize="15sp"
                android:textStyle="bold"></TextView>

            <ImageButton
                android:id="@+id/btnContacto"
                android:layout_width="56dp"
                android:layout_height="114dp"
                android:layout_marginLeft="20dp"
                android:layout_weight="0.15"
                android:background="@null"
                android:paddingBottom="25dp"
                android:src="@drawable/contacto"></ImageButton>

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="-75dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="30dp"
                android:clickable="false"
                android:text="CONTACTO"
                android:textColor="@color/Green"
                android:textSize="15sp"
                android:textStyle="bold"></TextView>


        </LinearLayout>

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dp"
            android:clickable="false"
            android:gravity="bottom"
            android:text="Linea1 Â©"
            android:textAlignment="center"
            android:textColor="@color/Black"
            android:textSize="10dp"></TextView>
    </LinearLayout>


</FrameLayout>
